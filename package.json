{
  "name": "utall",
  "version": "1.0.0",
  "description": "⚙️ Utility library ⚙️",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "run-p build:node build:browser",
    "build:browser": "tsc --project tsconfig.browser.json",
    "build:node": "tsc --project tsconfig.node.json",
    "lint": "eslint .",
    "prebuild": "rimraf dist/**/*",
    "prepublishOnly": "run-p lint test build",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blissful89/utall.git"
  },
  "author": "Marco van Harten",
  "license": "MIT",
  "keywords": [
    "async",
    "asynchronous",
    "browser",
    "monad",
    "node",
    "promise",
    "utility",
    "util"
  ],
  "bugs": {
    "url": "https://github.com/Blissful89/utall/issues"
  },
  "homepage": "https://github.com/Blissful89/utall#readme",
  "engines": {
    "node": ">= 8",
    "yarn": ">= 1"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
